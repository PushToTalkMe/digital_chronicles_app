{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "electron/main.js",
  "description": "A hackathon app",
  "author": "qjalti",
  "scripts": {
    "dev": "vite --host",
    "dev:all": "concurrently \"npm run dev:vite\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "^7.0.0-beta.17",
    "@mui/material": "^7.3.2",
    "axios": "^1.12.2",
    "concurrently": "^9.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "wait-on": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@yandex/ymaps3-types": "^1.0.17595433",
    "cross-env": "^10.0.0",
    "electron": "^34.0.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2"
  },
  "build": {
    "electronVersion": "34.0.1",
    "appId": "com.digitalChronicles.app",
    "productName": "DigitalChrinclesApp",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "compression": "maximum",
    "asar": true,
    "files": [
      "dist/**/*",
      "electron/**/*"
    ]
  }
}
